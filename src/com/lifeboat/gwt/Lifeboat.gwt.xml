<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="lifeboat">

    <inherits name='com.google.gwt.user.User'/>
    <inherits name="net.zschech.gwt.comet.Comet"/>
    <inherits name="com.extjs.gxt.ui.GXT"/>
    <inherits name="net.auroris.ColorPicker.client"/>

    <entry-point class='com.lifeboat.gwt.client.LifeboatEntryPoint'/>

    <servlet path="/StartGameService" class="com.lifeboat.gwt.server.StartGameServiceImpl"/>
    <servlet path="/PlayGameService" class="com.lifeboat.gwt.server.PlayGameServiceImpl"/>

</module>